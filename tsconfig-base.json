{
    "$schema": "http://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "baseUrl": ".",
        "rootDir": ".",
        "moduleResolution": "Node",
        "allowJs": false,
        "strictNullChecks": true,
        "downlevelIteration": true,
        "skipLibCheck": true,
        "module": "CommonJS",
        "target": "es2022",
        "types": [],
        "typeRoots": [],
        "paths": {
            "@/types/*": ["src/types/*"],
            "@/main/*": ["src/main/*"],
            "@/sub/*": ["src/subpackage/*"]
        }
    }
}
