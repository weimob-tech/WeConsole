<view class="action-list">
    <view class="action" bind:tap="callWxMethod" data-method="console">随机console</view>
    <view class="action" bind:tap="callWxMethod" data-method="showToast">swan.showToast</view>
    <view class="action" bind:tap="callWxMethod" data-method="setStorageSync">swan.setStorageSync</view>
    <view class="action" bind:tap="callWxMethod" data-method="getStorage">swan.getStorage</view>
    <view class="action" bind:tap="callWxMethod" data-method="removeStorage">swan.removeStorage</view>
    <view class="action {{requesting?'lock':''}}" bind:tap="callWxMethod" data-method="request">swan.request{{requesting?'(发送中...)':''}}</view>
</view>