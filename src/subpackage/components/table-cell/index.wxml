<block wx:if="{{value.tableCell}}">
    <view wx:for="{{value.blocks}}" wx:key="index" wx:for-item="block">
        <block wx:for="{{block.items}}" wx:key="index">
            <text wx:if="{{item.type==='text'}}" style="{{item.style}}">{{item.content}}</text>
            <json-viewer wx:elif="{{item.type==='json'}}" from="{{from}}_{{index}}" mode="{{2}}"
                init="{{false}}"></json-viewer>
            <text wx:else>{{item}}</text>
        </block>
    </view>
</block>
<block wx:else>{{value}}</block>